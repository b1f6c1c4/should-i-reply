{
    "manifest_version": 2,
    "name":    "Should I reply? - Telegram version",
    "version": "0.1",
    "content_scripts": [{
        "matches": ["*://web.telegram.org/*"],
        "js":      ["content.js"],
        "run_at": "document_start"
    }],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": ["activeTab"],

    "icons": { "48": "icon.png" }
}
